    "nums = [-100000,100000]\n",
    "count = 0\n",
    "addn = sum(nums)\n",
    "\n",
    "for i in range(len(nums)):\n",
    "    if abs(sum(nums[:i])) < abs(addn - sum(nums[:i])):  \n",
    "        count += 1\n",
    "        \n",
    "\n",
    "print(count)"
